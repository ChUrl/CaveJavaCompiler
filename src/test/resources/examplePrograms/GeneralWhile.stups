class WhileClass {

    public static void main(String[] args) {
        int a = 1;
        int b = 1;
        int temp = 0;

        while (a < 144) {
            temp = b;
            b = a + b;
            a = temp;

            System.out.println(a);
        }

        while (b < 1)
            temp = 55;

    }
}
