class IfElseClass {

    public static void main(String[] args) {
        int x = 5;
        int y = 10;
        if (x < y) {
            System.out.println("x ist kleiner als y.");
        } else {
            if (x > y) {
                System.out.println("x ist groesser als y.");
            }
        }
        System.out.println("x und y sind gleich gross.");

        if (1 < 2)
            String s = "Ohne Klammern, da 1 Line.";

        if (1 < 2)
            s = "If 1";
        if (2 < x)
            s = "If 2";
        else
            s = "Dangling else";
    }
}
